{
  "name": "inote",
  "version": "1.0.0",
  "description": "Interactive Note Management System with Context-Aware Commands",
  "private": true,
  "scripts": {
    "test": "npm run test:all --prefix android/native-gradle-node-folder/app/src/main/assets/nodejs-project",
    "test:quick": "npm run test:quick --prefix android/native-gradle-node-folder/app/src/main/assets/nodejs-project",
    "test:context": "npm run test:context --prefix android/native-gradle-node-folder/app/src/main/assets/nodejs-project",
    "test:all": "npm run test:all --prefix android/native-gradle-node-folder/app/src/main/assets/nodejs-project",
    "install:nodejs": "npm install --prefix android/native-gradle-node-folder/app/src/main/assets/nodejs-project",
    "start": "node android/native-gradle-node-folder/app/src/main/assets/nodejs-project/min.js",
    "build": "gradle-wrapper clean assembleDebug",
    "build:unix": "cd android/native-gradle-node-folder && ./gradlew clean assembleDebug",
    "build:windows": "cd android/native-gradle-node-folder && gradlew.bat clean assembleDebug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuri_appsfrnd/inote.git"
  },
  "keywords": [
    "notes",
    "nodejs",
    "websocket",
    "context-aware",
    "commands"
  ],
  "author": "yuri_appsfrnd",
  "license": "MIT"
}
